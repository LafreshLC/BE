{
    "compilerOptions": {
      "module": "CommonJS",
      "noEmitOnError": false,
      "target": "ES2016",
      "noImplicitAny": true,
      "removeComments": true,
      "strict": true,
      "esModuleInterop": true,
      "skipDefaultLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "rootDir": "./src",
      "outDir": "./dist",
      "baseUrl": ".",
      "paths":{
        "#/*": ["./src/*"]
      }
    }
  }
